{
  "name": "pupbot-plugin-music",
  "version": "1.0.1",
  "description": "QQ 机器人框架 pupbot 的点歌插件，支持六大主流平台的歌曲分享和查歌词的功能",
  "main": "lib/index.js",
  "author": "Viki <hi@viki.moe> (https://github.com/vikiboss)",
  "scripts": {
    "dev": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "build": "tsc && tsc-alias",
    "test": "ava",
    "clean": "rimraf lib",
    "release": "npm run clean && npm run build && npm publish"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "pupbot",
    "pupbot-plugin"
  ],
  "license": "MIT",
  "devDependencies": {
    "@pupbot/core": "latest",
    "ava": "^5.1.0",
    "concurrently": "^7.6.0",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.8.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "html-entities": "^2.3.3"
  }
}
